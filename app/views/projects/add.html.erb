<h1>New Project</h1>

<%= form_for :project,  url: projects_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :owner %><br>
    <%= f.text_field :owner %>
  </p>

  <!-- <p>
    <%= f.label :deadline %><br>
    <%= f.text_field :deadline %>
  </p> -->

  <p>
    <%= f.submit %>
  </p>
<% end %>
